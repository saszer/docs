                    "confidence": {
                      "type": "number",
                      "minimum": 0,
                      "maximum": 1
                    },
                    "isTaxDeductible": {
                      "type": "boolean"
                    },
                    "taxCategory": {
                      "type": "string"
                    },
                    "reasoning": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
